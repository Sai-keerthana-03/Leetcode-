class Solution {
    ArrayList<Integer> l = new ArrayList<>();
    public void post(TreeNode root){
        if(root == null){
            return;
        }
        post(root.left);
        post(root.right);
        l.add(root.val);
    }
    public List<Integer> postorderTraversal(TreeNode root) {
        post(root);
        return l;
    }
}
