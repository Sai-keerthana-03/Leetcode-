class Solution {
    ArrayList<Integer> l = new ArrayList<>();
        public void in(TreeNode root){
            if(root==null){
                return;
            }
            in(root.left);
            l.add(root.val);
            in(root.right);
        } 
    public List<Integer> inorderTraversal(TreeNode root) {
        in(root);
        return l;
    }
}
